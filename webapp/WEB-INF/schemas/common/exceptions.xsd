<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="urn://nahodka.ru/services/common/schemas/exceptions/1.0.0"
        elementFormDefault="qualified"
        xmlns="http://www.w3.org/2001/XMLSchema"
        xmlns:tns="urn://nahodka.ru/services/common/schemas/exceptions/1.0.0">

    <simpleType name="ErrorCode">
        <annotation>
            <documentation>Код ошибки</documentation>
        </annotation>

        <restriction base="string">
            <annotation>
                <documentation>Перечисление возможных обрабатываемых кодов ошибок сервиса</documentation>
            </annotation>

            <enumeration value="NON_UNIQUE_DATA_SOURCE_CODE">
                <annotation>
                    <documentation>Обнаружено неуникальное значение </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_DIMENSIONS_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент Dimensions </documentation>
                </annotation>
            </enumeration>
            <enumeration value="WRONG_REGION_EXCEPTION">
                <annotation>
                    <documentation>Недопустимое значение региона</documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_APPEAL_REQUEST_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует запрос на апелляцию APPEAL REQUEST </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_APPEAL_CANCEL_REQUEST_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует запрос на отмену апелляции APPEAL CANCEL REQUEST </documentation>
                </annotation>
            </enumeration>
            <enumeration value="NOT_FOUND">
                <annotation>
                    <documentation>Результат не найден</documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_DICTIONARY_CONTENT_REQUEST">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент DICTIONARY_CONTENT_REQUEST</documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_REQUEST">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент NEW_REQUEST</documentation>
                </annotation>
            </enumeration>
            <enumeration value="UNKNOWN_REQUEST_EXCEPTION">
                <annotation>
                    <documentation>Тип запроса неизвестен</documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_SERVICE_CODE_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент SERVICE_CODE </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_SERVICE_NAME_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент SERVICE_NAME </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_ID_APPLICATION_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент ID_APPLICATION </documentation>
                </annotation>
            </enumeration>
            <enumeration value="WRONG_DATE">
                <annotation>
                    <documentation>Недопустимое значение DATE_APPLICATION </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_DATE_APPLICATION_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент DATE_APPLICATION </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_APP_TYPE_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент APP_TYPE </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_SNILS_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент SNILS</documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_LAST_NAME_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент LAST_NAME и LAST_NAME_GR </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_FIRST_NAME_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент FIRST_NAME и FIRST_NAME_GR </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_PASSPORT_SERIES_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент PAS_SER и PAS_SER_GR </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_PASSPORT_NUMBER_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент PAS_NUM и PAS_NUM_GR </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_PASSPORT_DATE_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент PAS_DATE и PAS_DATE_GR </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_PASSPORT_ORG_EXCEPTION">
                <annotation>
