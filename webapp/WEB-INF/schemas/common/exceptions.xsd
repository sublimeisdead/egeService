                </annotation>
            </enumeration>
            <enumeration value="EMPTY_APPLICANT_EQUALS_EXAMINEE_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент APPLICANT_EQUALS_EXAMINEE </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_YEAR_EXAM_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент YEAR_EXAM </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_CODE_SUBJECT_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент CODE_SUBJECT </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_SUBJECT_TEXT_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент SUBJECT_TEXT </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_EXAM_DATE_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент EXAM_DATE </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_CODE_SCHOOL_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент CODE_SCHOOL </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_SCHOOL_TEXT_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент SCHOOL_TEXT </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_CODE_PLACE_EXAM_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент CODE_PLACE_EXAM </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_PLACE_EXAM_EXCEPTION">
                <annotation>
                    <documentation>Отсутствует, либо пуст обязательный элемент PLACE_EXAM </documentation>
                </annotation>
            </enumeration>
            <enumeration value="EMPTY_PRESENCE_EXCEPTION">
